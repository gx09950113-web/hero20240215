{
  "植物分類": [
    {
      "名稱": "苔蘚植物",
      "特徵": "無維管組織，沒有真正的根、莖、葉，通過孢子繁殖，需在潮濕環境下生長。",
      "代表": ["蘚類（如葫蘆蘚）", "地錢（苔類）", "角苔"]
    },
    {
      "名稱": "蕨類植物",
      "特徵": "具有維管組織（運輸水分和養分），通過孢子繁殖，通常有分化的根、莖和葉。",
      "代表": ["蕨類（如桫欏、鐵線蕨）", "石松", "卷柏"]
    },
    {
      "名稱": "裸子植物",
      "特徵": "種子裸露，沒有果皮包裹，常綠且耐寒，通常為木本植物。",
      "代表": ["松樹", "杉樹", "銀杏", "蘇鐵"]
    },
    {
      "名稱": "被子植物",
      "細分": ["草本植物", "木本植物", "多肉植物"],
      "特徵": "具有果實包裹種子，是植物界中最多樣化、進化程度最高的類群。",
      "代表": [
        "草本植物（如小麥、稻米）",
        "木本植物（如橡樹、櫻花）",
        "多肉植物"
      ]
    },
    {
      "名稱": "藻類植物",
      "特徵": "沒有根、莖、葉，簡單的光合作用植物，主要生活在水中。",
      "代表": ["海帶", "螺旋藻", "綠藻", "紅藻"]
    }
  ]
}
